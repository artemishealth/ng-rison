{
  "name": "ng-rison",
  "version": "0.1.0",
  "main": "./bundles/ng-rison.umd.js",
  "module": "./dist/ng-rison.es5.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist bundles",
    "start": "npm run build && npm run bundle",
    "watch": "watch --wait=5 'npm run start' src/",
    "build": "ngc -p src/tsconfig.app.json",
    "bundle": "npm run bundle:es5 && npm run bundle:umd && npm run minify:umd",
    "bundle:es5": "rollup -c config/rollup-es5.config.js --silent",
    "bundle:umd": "rollup -c config/rollup-umd.config.js --silent",
    "minify:umd": "terser bundles/$npm_package_name.umd.js -o bundles/$npm_package_name.umd.min.js --source-map=\"filename=bundles/$npm_package_name.umd.min.js.map\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "core-js": "~3.6.5",
    "tslib": "~2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.0.3",
    "@angular/cli": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "codelyzer": "~6.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "rollup": "~1.7.3",
    "rollup-plugin-node-resolve": "~4.0.1",
    "rollup-plugin-sourcemaps": "~0.4.2",
    "terser": "~3.17.0",
    "ts-node": "~9.0.0",
    "tsickle": "~0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "watch": "~1.0.2"
  }
}
